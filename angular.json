      }
    },
    "auth": {
      "projectType": "library",
      "root": "libs/auth",
      "sourceRoot": "libs/auth/src",
      "prefix": "webrtc-demos",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
